   "source": [
    "def twice(f):\n",
    "    def result(x):\n",
    "        return f(f(x))\n",
    "    return result\n",
    "\n",
    "def f(i):\n",
    "    return i + 3\n",
    "\n",
    "g1 = twice(f)"
